//The default page for looking up student.
extend default
			
block content
	.data-report-page
		.form
			form.data-report(action='/back_homepage', method = 'GET')
				input(type='text', name='f_name', placeholder='Student\'s First Name')
				input(type='text', name='l_name', placeholder='Student\'s Last Name')
				input(type='text', name= 'st_id', placeholder='Student ID')
				input(type='text', name= 'degree', placeholder='Degree')
				input(type='text', name= 'degree_level', placeholder='Degree Level')
				input(type='text', name= 'grad_term', placeholder='Graduation Term')
				input(type='text', name= 'grad_year', placeholder='Graduation Year')
				input(type='text', name= 'ext_mail', placeholder='External E-Mail')
				input(type='text', name= 'uw_mail', placeholder='UW E-Mail')
				input(type='text', name= 'gpa', placeholder='GPA')
				input(type='text', name='job_title',placeholder='Job Title')
				input(type='text', name= 'skills', placeholder='Skills')
				input(type='text', name= 'salary', placeholder='Salary')
				button(onclick="move(); type='submit', return false;") Homepage
