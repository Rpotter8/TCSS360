extend default
block content
	.main_content
		.main-page
			.form
				form.select-form
					button(onclick="add_move(); return false;") Add Student
					button(onclick="edit_move(); return false;") Edit Student
					button(onclick="delete_move(); return false;") Delete Student
					button(onclick="student_report_move(); return false;") Job Report
					button(onclick="gpa_report_move(); return false;") GPA Report
					button(onclick="list_move(); return false;") List Students
					button(onclick="lookup_student_move(); return false;") Lookup Student
					button(onclick="add_job_move(); return false;") Add Job
					button(onclick="add_degree_move(); return false;") Add Degree
					button(onclick="add_skill_move(); return false;") Add Skill